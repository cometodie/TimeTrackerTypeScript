{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "lib": ["es6", "dom"],
    "jsx": "react",
    "baseUrl": ".",
    // "pretty": true,
    "paths": {
      "actions/*": ["app/actions/*"],
      "dbApi/*": ["app/dbApi/*"],
      "components/*": ["app/components/*"],
      "config/*": ["config/*"],
      "containers/*": ["app/containers/*"],
      "constants/*": ["constants/*"],
      "models/*": ["app/models/*"],
      "utilities/*": ["app/components/utilities/*"],
      "store/*": ["app/store/*"],
    }
  },
  "exclude": ["dist", "node_modules"],
  // "include": ["app/**/*"]
}
